<h2><%= t('.header') %></h2>
<% if presenter.member_presenters.present? %>
  <table class="table table-striped related-files datatable">
    <thead>
      <tr>
        <th><%= t('.folder_number') %></th>
        <th><%= t('.workflow_state') %></th>
        <th><%= t('.title') %></th>
        <th><%= t('.barcode') %></th>
        <th><%= t('.genre') %></th>
        <th><%= t('.actions') %></th>
      </tr>
    </thead>
    <tbody>
      <%= render partial: 'member', collection: presenter.member_presenters.sort_by(&:folder_number), locals: { box: presenter } %>
    </tbody>
  </table>
<% elsif can? :edit, presenter.id %>
    <div class="alert alert-warning" role="alert"><%= t('.empty', type: presenter.human_readable_type) %></div>
<% end %>
