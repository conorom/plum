<% if f.object.multiple? key %>
  <%= f.input key, as: :multi_value, input_html: { class: 'form-control' }, required: f.object.required?(key), autofocus: f.object.try(:autofocus_barcode?) %>
<% else %>
  <%= f.input key, required: f.object.required?(key), autofocus: f.object.try(:autofocus_barcode?) %>
<% end %>
